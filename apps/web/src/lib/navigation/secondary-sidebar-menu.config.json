{
  "version": "1.0.0",
  "lastUpdated": "2026-01-16",
  "contexts": {
    "chat": {
      "id": "chat",
      "label": "Chat History",
      "component": "ChatList",
      "header": {
        "title": "Chats",
        "actionButton": {
          "label": "New Chat",
          "icon": "Plus",
          "action": "createNewChat",
          "primary": true
        }
      },
      "categories": [
        {
          "id": "recent",
          "label": "Recent",
          "type": "dynamic",
          "icon": "Clock",
          "defaultOpen": true,
          "limit": 50
        },
        {
          "id": "folders",
          "label": "Folders",
          "type": "folders",
          "icon": "Folder",
          "defaultOpen": true,
          "items": [
            {
              "id": "work",
              "label": "Work",
              "icon": "Briefcase",
              "count": 0
            },
            {
              "id": "personal",
              "label": "Personal",
              "icon": "User",
              "count": 0
            }
          ]
        }
      ],
      "search": {
        "placeholder": "Search chats...",
        "icon": "Search",
        "filters": ["tags", "folders", "date"]
      },
      "itemActions": [
        {
          "id": "pin",
          "label": "Pin",
          "icon": "Pin",
          "action": "pinChat"
        },
        {
          "id": "rename",
          "label": "Rename",
          "icon": "Pencil",
          "action": "renameChat"
        },
        {
          "id": "delete",
          "label": "Delete",
          "icon": "Trash2",
          "action": "deleteChat",
          "destructive": true
        }
      ]
    },
